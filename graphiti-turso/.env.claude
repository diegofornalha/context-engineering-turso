# Configuração do Graphiti com Claude Nativo
# Não precisa de API keys da OpenAI!

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=demodemo

# Claude Configuration (usa Claude nativo do Claude Code)
USE_CLAUDE_NATIVE=true
CLAUDE_MODEL=claude-3-5-sonnet-20241022
CLAUDE_MAX_TOKENS=4096
CLAUDE_TEMPERATURE=0.7

# Graphiti Settings
SEMAPHORE_LIMIT=10
DEFAULT_GROUP_ID=default

# Server Settings
MCP_TRANSPORT=stdio
MCP_PORT=8000

# Logging
LOG_LEVEL=INFO