{
  "name": "context-engineering-turso-a2a",
  "version": "1.0.0",
  "description": "Context Engineering Turso with A2A Protocol Support",
  "main": "a2a-server.js",
  "scripts": {
    "start": "node a2a-server.js",
    "dev": "nodemon a2a-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "prp-agent": "cd prp-agent && python main.py",
    "turso-agent": "cd turso-agent && python main.py"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "axios": "^1.6.0"
  }
}
